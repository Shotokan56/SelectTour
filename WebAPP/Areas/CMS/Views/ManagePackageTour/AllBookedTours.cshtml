@model IEnumerable<WebAPP.Areas.CMS.Models.PackageTourBookedViewModel>

@{
    ViewBag.Title = "Booked Tour";
    Layout = "~/Areas/CMS/Views/Shared/_Layout_CMS.cshtml";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DepartureDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TourClass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactName)
        </th>
        <th>Action</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepartureDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TourClass)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactName)
            </td>
            <td>
                @Html.ActionLink("Details", "DetailsBooked", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }

</table>
